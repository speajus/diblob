{
  "name": "@speajus/diblob-svelte",
  "version": "1.0.1",
  "description": "Svelte integration helpers for diblob dependency injection containers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speajus/diblob.git"
  },
  "bugs": {
    "url": "https://github.com/speajus/diblob/issues"
  },
  "homepage": "https://speajus.github.io/diblob/",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build:all": "pnpm --filter @speajus/diblob build && pnpm --filter @speajus/diblob-mcp build && pnpm --filter @speajus/diblob-connect build && pnpm --filter @speajus/diblob-visualizer build && pnpm --filter @speajus/diblob-svelte build",
    "dev": "tsc --watch",
    "test": "pnpm run build && tsx --test test/*.test.ts",
    "test:watch": "tsx --test --watch test/*.test.ts"
  },
  "keywords": [
    "svelte",
    "dependency-injection",
    "diblob",
    "di",
    "context",
    "container"
  ],
  "author": "Justin Spears <speajus@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@speajus/diblob": "workspace:*"
  },
  "peerDependencies": {
    "svelte": "^5.39.6"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "svelte": "^5.39.6",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3"
  }
}
