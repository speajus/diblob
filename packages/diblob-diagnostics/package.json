{
  "name": "@speajus/diblob-diagnostics",
  "version": "1.0.2",
  "description": "Diagnostics helper for diblob containers (LLM-friendly snapshots of logs and metrics)",
  "author": "Justin Spears <speajus@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
