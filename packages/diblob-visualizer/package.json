{
  "name": "@speajus/diblob-visualizer",
  "private": false,
  "version": "0.2.1",
  "type": "module",
  "description": "Interactive dependency injection graph visualization for diblob",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "diblob-visualizer": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/express.d.ts",
      "default": "./dist/middleware/express.js"
    },
    "./web-component": {
      "types": "./dist/web-component/index.d.ts",
      "default": "./dist/web-component/web-component.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "!dist/cli/index.html"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:cli && npm run build:webcomponent && npm run build:server && npm run build:middleware",
    "build:lib": "vite build",
    "build:cli": "vite build --config vite.config.cli.ts && mkdir -p dist/cli-assets && cp -r dist/cli/* dist/cli-assets/ && npx tsc src/cli/index.ts --outDir dist/cli --module esnext --target es2022 --moduleResolution bundler --skipLibCheck --esModuleInterop && chmod +x dist/cli/index.js",
    "build:webcomponent": "vite build --config vite.config.webcomponent.ts && cp src/web-component/index.d.ts dist/web-component/",
    "build:server": "npx tsc -p tsconfig.server.json",
    "build:middleware": "npx tsc -p tsconfig.middleware.json",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "server": "tsx example-server.ts"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/express": "^4.17.25",
    "@types/node": "^24.6.0",
    "svelte": "^5.39.6",
    "svelte-check": "^4.3.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@speajus/diblob": "*",
    "@xyflow/svelte": "^1.4.1"
  },
  "peerDependencies": {
    "express": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}
