{
  "name": "@speajus/diblob-visualizer",
  "private": false,
  "version": "1.0.2",
  "type": "module",
  "description": "Interactive dependency injection graph visualization for diblob",
  "author": "Justin Spears <speajus@gmail.com>",
  "license": "MIT",
  "main": "./dist/server/src/server/index.js",
  "types": "./dist/types/src/server/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/lib/index.d.ts",
      "svelte": "./src/lib/index.ts",
      "default": "./dist/svelte/index.js"
    },
    "./server": {
      "types": "./dist/types/src/server/index.d.ts",
      "default": "./dist/server/src/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite --mode demo",
    "build": "vite build && tsc -p tsconfig.server.json && tsc -p tsconfig.types.json",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "test": "pnpm run build && tsx --test test/*.test.ts",
    "test:watch": "tsx --test --watch test/*.test.ts",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "server": "tsx example-server.ts"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.10.0",
    "svelte": "^5.39.6",
    "svelte-check": "^4.3.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@speajus/diblob": "workspace:*",
    "@xyflow/svelte": "^1.4.1"
  }
}
