{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
		"baseUrl": "./",
		"paths": {
			"@speajus/diblob": ["packages/diblob/src"],
			"@speajus/diblob/*": ["packages/diblob/src/*"]
		},
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true
  }
}
